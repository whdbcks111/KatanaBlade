@inherits Umbraco.Web.Mvc.UmbracoViewPage<BaseModel>
@{
    Layout = "Base.cshtml";
}
<div class="content-wrap">
    <div class="container-fluid">
        <section class="left">
            @if (Model.Content.Level > 2)
            {
                <h2>@Model.Content.Name</h2>
            }

            @Model.Content.GetGridHtml("content")
        </section>
        <aside class="right">
            @{Html.RenderPartial("layout/SideNavigation", Model);}
        </aside>
    </div>
</div>
