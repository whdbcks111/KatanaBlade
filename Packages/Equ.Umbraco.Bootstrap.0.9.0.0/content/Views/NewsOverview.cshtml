@model NewsOverviewViewModel
@{
    Layout = "Base.cshtml";
}


@foreach (var item in Model.NewsList)
{
    <article class="news">
        <header>
            <h3><a href="@item.NiceUrl">@item.Name</a></h3>
            <div class="date"><time pubdate datetime="@item.NewsDate">@item.NewsDate.ToShortDateString()</time></div>

        </header>
        <div>
            
        </div>
    </article>
}

@Html.Partial("Pagination", Model.Pager)